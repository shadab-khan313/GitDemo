package com.syn.examples;

class Overloaded {

	public void add(int a, int b) {

		System.out.println("from 1st methoed :: " + (a + b));
	}

	public void add(long a, int b) {

		System.out.println("from 1st methoed :: " + (a + b));
		//return 0;
	}
}

public class OverloadingTestDemo {

	public static void main(String[] args) {

	}

}
