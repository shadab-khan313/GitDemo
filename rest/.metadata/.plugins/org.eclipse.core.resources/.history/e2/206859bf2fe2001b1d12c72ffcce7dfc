package com.syn.examples;

class A{
	
	public void add(int a, int b) {
		System.out.println(a+b);
	}
	
}

class B extends A{
	
	public void sub(int a,int b) {
		System.out.println(a-b);
	}
}

public class OverridingTestDemo {
	
	public static void main(String[] args) {
		
		A b = new B();
		b.add(10,20);
	}
	
	

}
